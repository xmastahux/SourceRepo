# Auto detect text files and perform LF normalization
* text=auto
.bin filter=lfs diff=lfs merge=lfs -text
.bmp filter=lfs diff=lfs merge=lfs -text
.lut filter=lfs diff=lfs merge=lfs -text
.fir filter=lfs diff=lfs merge=lfs -text
.rbf filter=lfs diff=lfs merge=lfs -text
.mo filter=lfs diff=lfs merge=lfs -text
.sym filter=lfs diff=lfs merge=lfs -text
.lua filter=lfs diff=lfs merge=lfs -text
.cfg filter=lfs diff=lfs merge=lfs -text
.set filter=lfs diff=lfs merge=lfs -text
.cfg filter=lfs diff=lfs merge=lfs -text
.len filter=lfs diff=lfs merge=lfs -text
.ds_store filter=lfs diff=lfs merge=lfs -text
.txt filter=lfs diff=lfs merge=lfs -text
